												<cedsURL/>
											</xs:appinfo>
											<xs:documentation/>
										</xs:annotation>
										<xs:complexType>
											<xs:attribute name="emailType" type="xs:string">
												<xs:annotation>
													<xs:appinfo>
														<sifChar>O</sifChar>
														<cedsId>000089</cedsId>
														<cedsURL>https://ceds.ed.gov/cedselementdetails.aspx?termid=3089</cedsURL>
													</xs:appinfo>
													<xs:documentation>The type of electronic mail (e-mail) address listed for a person or organization.</xs:documentation>
												</xs:annotation>
