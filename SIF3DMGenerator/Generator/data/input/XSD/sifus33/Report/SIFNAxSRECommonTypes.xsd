<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:include schemaLocation="../Composite/SIFNAxPressCommonTypes.xsd"/>
	<xs:include schemaLocation="../Composite/SIFNAxPress.xsd"/>
	<xs:complexType name="xTranscriptTermType">
		<xs:sequence>
			<xs:element name="school" type="xSchoolType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The school of instruction for this transcript term.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="schoolYear" type="xSchoolYearType" minOccurs="0">
				<xs:annotation>
					<xs:appinfo>
						<elementName>School Year</elementName>
						<sifChar>O</sifChar>
						<cedsId>000243</cedsId>
						<cedsURL>https://ceds.ed.gov/cedselementdetails.aspx?termid=3243</cedsURL>
					</xs:appinfo>
					<xs:documentation>The year for a reported school term/session.  For academic years that span a calendar year this is the four digit year-end. E.g. 2013 for 2012-2013.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="session" type="xSessionType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The school term/session to which this transcript term belongs.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="gradeLevel" type="xGradeLevelType" minOccurs="0">
				<xs:annotation>
					<xs:appinfo>
						<elementName>Grade Level When Course Taken</elementName>
						<sifChar>O</sifChar>
						<cedsId>000125</cedsId>
						<cedsURL>https://ceds.ed.gov/cedselementdetails.aspx?termid=3125</cedsURL>
					</xs:appinfo>
					<xs:documentation>Student's grade level at time of courses in term.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="courses" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Courses taken, attempted or completed, during the term/session.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="course" type="xSreCourseType" minOccurs="0" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="academicSummary" type="xSreAcademicSummaryType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Academic summary metrics for the student during this tem/session.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:any namespace="##other" processContents="strict" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="xSreCourseType">
		<xs:complexContent>
			<xs:extension base="xCourseType">
				<xs:sequence>
					<xs:element name="creditsAttempted" type="xs:decimal" minOccurs="0">
						<xs:annotation>
							<xs:appinfo>
								<elementName>Number of Credits Attempted</elementName>
								<sifChar>O</sifChar>
								<cedsId>000199</cedsId>
								<cedsURL>https://ceds.ed.gov/cedselementdetails.aspx?termid=3199</cedsURL>
							</xs:appinfo>
							<xs:documentation>The number of credits that a student can earn for enrolling in and completing a given course.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="creditsEarned" type="xs:decimal" minOccurs="0">
						<xs:annotation>
							<xs:appinfo>
								<elementName>Number of Credits Earned</elementName>
								<sifChar>O</sifChar>
								<cedsId>000200</cedsId>
								<cedsURL>https://ceds.ed.gov/cedselementdetails.aspx?termid=3200</cedsURL>
							</xs:appinfo>
							<xs:documentation>The number of credits a student earned for completing a given course.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="creditTypeEarned" type="xCreditTypeEarnedType" minOccurs="0">
						<xs:annotation>
							<xs:appinfo>
								<elementName>Credit Type Earned</elementName>
								<sifChar>O</sifChar>
								<cedsId>000072</cedsId>
								<cedsURL>https://ceds.ed.gov/cedselementdetails.aspx?termid=3072</cedsURL>
							</xs:appinfo>
							<xs:documentation>The type of credits or units of value awarded for the completion of a course.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="progressGPA" type="xs:decimal" minOccurs="0">
						<xs:annotation>
							<xs:appinfo>
								<elementName>Progress GPA Mark Value</elementName>
								<sifChar>O</sifChar>
								<cedsId/>
								<cedsURL/>
							</xs:appinfo>
							<xs:documentation>The numeric GPA value of the progress mark.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="progressMark" type="xs:token" minOccurs="0">
						<xs:annotation>
							<xs:appinfo>
								<elementName>Mid Term Mark</elementName>
								<sifChar>O</sifChar>
								<cedsId>000183</cedsId>
								<cedsURL>https://ceds.ed.gov/cedselementdetails.aspx?termid=3183</cedsURL>
							</xs:appinfo>
							<xs:documentation>Indicator of student performance at the mid-point of the marking period.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="gpaWeightedMark" type="xs:decimal" minOccurs="0">
						<xs:annotation>
							<xs:appinfo>
								<elementName>Weighted GPA Final Mark</elementName>
								<sifChar>O</sifChar>
							</xs:appinfo>
							<xs:documentation>The numeric locally weighted GPA value of the final mark.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="gpaUnweightedMark" type="xs:decimal" minOccurs="0">
						<xs:annotation>
							<xs:appinfo>
								<elementName>Unweighted GPA Final Mark</elementName>
								<sifChar>O</sifChar>
							</xs:appinfo>
							<xs:documentation>The numeric unweighted GPA value of the final mark.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="finalMarkValue" type="xs:token" minOccurs="0">
						<xs:annotation>
							<xs:appinfo>
								<elementName>Grade Earned</elementName>
								<sifChar>O</sifChar>
								<cedsId>000124</cedsId>
								<cedsURL>https://ceds.ed.gov/cedselementdetails.aspx?termid=3124</cedsURL>
							</xs:appinfo>
							<xs:documentation>A final indicator of student performance in a class as submitted by the instructor.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="classSectionId" type="xs:token" minOccurs="0">
						<xs:annotation>
							<xs:appinfo>
								<elementName>Class Section Identifier</elementName>
								<sifChar>O</sifChar>
								<cedsId>000978</cedsId>
								<cedsURL>https://ceds.ed.gov/cedselementdetails.aspx?termid=3979</cedsURL>
							</xs:appinfo>
							<xs:documentation>A unique number or alphanumeric code assigned to an institution by a school, school system, a state, or other agency or entity for a particular class-section.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="courseLevelCharacteristic" type="xCourseLevelCharacteristicType" minOccurs="0">
						<xs:annotation>
							<xs:appinfo>
								<sifChar>O</sifChar>
								<cedsId>000061</cedsId>
								<cedsURL>https://ceds.ed.gov/cedselementdetails.aspx?termid=20298</cedsURL>
							</xs:appinfo>
							<xs:documentation>An indication of the general nature and difficulty of instruction provided throughout a course.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="xCompetencyType">
		<xs:sequence>
			<xs:element name="learningStandardItemUrl" type="xs:anyURI">
				<xs:annotation>
					<xs:appinfo>
						<elmentName>LearningStandardItemUrl</elmentName>
						<sifChar>O</sifChar>
						<cedsId>000874</cedsId>
						<cedsURL>https://ceds.ed.gov/CEDSElementDetails.aspx?TermxTopicId=25325</cedsURL>
					</xs:appinfo>
					<xs:documentation>A network-resolvable Uniform Resource Locator (URL) pointing to the authoritative reference for the learning standard item.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="learningStandardItemCode" type="gLaxCodedElementType" minOccurs="0">
				<xs:annotation>
					<xs:appinfo>
						<elementName>Learning Standard Item Code</elementName>
						<sifChar>O</sifChar>
						<cedsId>000692</cedsId>
						<cedsURL>https://ceds.ed.gov/cedselementdetails.aspx?termid=3669</cedsURL>
					</xs:appinfo>
					<xs:documentation>A human-referenceable code designated by the state or other organization to identify the item in the hierarchy of learning standard items.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="masteryLevel" type="xs:token" minOccurs="0">
				<xs:annotation>
					<xs:appinfo>
						<elementName>Achievement Category Type</elementName>
						<sifChar>O</sifChar>
						<cedsId>000892</cedsId>
						<cedsURL>https://ceds.ed.gov/cedselementdetails.aspx?termid=3892</cedsURL>
					</xs:appinfo>
					<xs:documentation>The category of achievement attributed to the learner.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="masteryLevelSystem" type="xs:normalizedString" minOccurs="0">
				<xs:annotation>
					<xs:appinfo>
						<elementName>Achievement Category System</elementName>
						<sifChar>O</sifChar>
						<cedsId>001245</cedsId>
						<cedsURL>https://ceds.ed.gov/cedselementdetails.aspx?termid=4211</cedsURL>
					</xs:appinfo>
					<xs:documentation>The system that defines the categories by which an achievement is attributed to the learner.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="associatedCourses" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The course(s) in which this competency was mastered or earned.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="courseRefId" minOccurs="0" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:any namespace="##other" processContents="strict" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="xSreAcademicSummaryType">
		<xs:annotation>
			<xs:documentation>Extends the basic xAcademicSummaryType to include more comprehensive academic summary information that may be necessary to complete a successful student record exchange.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="xAcademicSummaryType">
				<xs:sequence>
					<xs:element name="achievements" minOccurs="0">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="achievement" type="xAchievementType" minOccurs="0" maxOccurs="unbounded"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="graduationRequirements" minOccurs="0">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="graduationRequirement" type="xGraduationRequirementType" minOccurs="0" maxOccurs="unbounded"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="gpaScale" type="xs:token" minOccurs="0">
						<xs:annotation>
							<xs:appinfo>
								<elementName>Grade Value Qualifier</elementName>
								<sifChar>O</sifChar>
								<cedsId>000616</cedsId>
								<cedsURL>https://ceds.ed.gov/cedselementdetails.aspx?termid=3609</cedsURL>
							</xs:appinfo>
							<xs:documentation>The scale of equivalents, if applicable, for grades awarded as indicators of performance in schoolwork. For example, numerical equivalents for letter grades used in determining a student's Grade Point Average (A=4, B=3, C=2, D=1 in a four-point system) or letter equivalents for percentage grades (90-100%=A, 80-90%=B, etc.).</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="termGpa" type="xs:decimal" minOccurs="0">
						<xs:annotation>
							<xs:appinfo>
								<elementName>GPA Given for Session</elementName>
								<sifChar>O</sifChar>
								<cedsId>000129</cedsId>
								<cedsURL>https://ceds.ed.gov/cedselementdetails.aspx?termid=3129</cedsURL>
							</xs:appinfo>
							<xs:documentation>An measure of average performance in all courses taken by a person during a given term/session. This is obtained by dividing the total unweighted grade points received by the number of credits attempted for the same session.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="cumulativeGpa" type="xs:decimal" minOccurs="0">
						<xs:annotation>
							<xs:appinfo>
								<elementName>Cumulative GPA</elementName>
								<sifChar>O</sifChar>
								<cedsId>000128</cedsId>
								<cedsURL>https://ceds.ed.gov/cedselementdetails.aspx?termid=3128</cedsURL>
							</xs:appinfo>
							<xs:documentation>A measure of average performance in all courses taken by a person during his or her school career as determined for record-keeping purposes. This is obtained by dividing the unweighted total grade points received by the total number of credits attempted. This usually includes grade points received and credits attempted in his or her current school as well as those transferred from schools in which the person was previously enrolled.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="gpaType" type="xs:normalizedString" minOccurs="0">
						<xs:annotation>
							<xs:appinfo>
								<elementName>GPA Type</elementName>
								<sifChar>O</sifChar>
							</xs:appinfo>
							<xs:documentation>A description of the GPA scale how it is totaled.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="termCreditsEarned" type="xs:decimal" minOccurs="0">
						<xs:annotation>
							<xs:appinfo>
								<elementName>Number of Credits Earned During Term</elementName>
								<sifChar>O</sifChar>
								<cedsId>000200</cedsId>
								<cedsURL>https://ceds.ed.gov/cedselementdetails.aspx?termid=3200</cedsURL>
							</xs:appinfo>
							<xs:documentation>The total number of credits a person earns by completing courses or examinations during a given term/session.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="termCreditsAttempted" type="xs:decimal" minOccurs="0">
						<xs:annotation>
							<xs:appinfo>
								<elementName>Number of Credits Attempted During Term</elementName>
								<sifChar>O</sifChar>
								<cedsId>000199</cedsId>
								<cedsURL>https://ceds.ed.gov/cedselementdetails.aspx?termid=3199</cedsURL>
							</xs:appinfo>
							<xs:documentation>The total number of credits a person attempts to earn by taking courses during a given term/session.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="totalCreditsEarned" type="xs:decimal" minOccurs="0">
						<xs:annotation>
							<xs:appinfo>
								<elementName>Total Number of Credits Earned</elementName>
								<sifChar>O</sifChar>
								<cedsId>000200</cedsId>
								<cedsURL>https://ceds.ed.gov/cedselementdetails.aspx?termid=3200</cedsURL>
							</xs:appinfo>
							<xs:documentation>The cumulative number of credits a person earns by completing courses or examinations during his or her enrollment in the current school as well as those credits transferred from schools in which the person had been previously enrolled.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="totalCreditsAttempted" type="xs:decimal" minOccurs="0">
						<xs:annotation>
							<xs:appinfo>
								<elementName>Total Number of Credits Attempted</elementName>
								<sifChar>O</sifChar>
								<cedsId>000199</cedsId>
								<cedsURL>https://ceds.ed.gov/cedselementdetails.aspx?termid=3199</cedsURL>
							</xs:appinfo>
							<xs:documentation>The cumulative number of credits a person attempts to earn by taking courses during his or her enrollment in the current school as well as those credits transferred from schools in which the person had been previously enrolled.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="classTotalNumber" type="xs:unsignedInt" minOccurs="0">
						<xs:annotation>
							<xs:appinfo>
								<elementName>Total Number in Class</elementName>
								<sifChar>O</sifChar>
								<cedsId>000294</cedsId>
								<cedsURL>https://ceds.ed.gov/cedselementdetails.aspx?termid=3294</cedsURL>
							</xs:appinfo>
							<xs:documentation>The total number of students in the student's high school graduating class.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="xAchievementType">
