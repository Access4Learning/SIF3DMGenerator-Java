	<xs:complexType name="xSchoolType">
		<xs:annotation>
			<xs:appinfo>
				<elementName>xSchool Type</elementName>
				<events>yes</events>
				<isSIFObject>yes</isSIFObject>
				<cedsId/>
				<cedsURL/>
				<servicePaths>					
					<servicePath>
						<path>xLeas/{}/xSchools</path>
						<description>Returns information for all schools in the LEA specified.</description>
						<xquery>
							<![CDATA[
							declare default element namespace "http://www.sifassociation.org/datamodel/na/3.3";
							/xSchool[leaRefId="{}"]
							]]>
						</xquery>
					</servicePath>
					<servicePath>
						<path>xStaffs/{}/xSchools</path>
						<description>Returns information for all schools associated with the staff specified.</description>
						<xquery>
							<![CDATA[
							declare default element namespace "http://www.sifassociation.org/datamodel/na/3.3";
							for $staff in /xStaff[@refId="{}"]
							for $school in /xSchool
							where $school/@refId = $staff/primaryAssignment/schoolRefId or $school/@refId = $staff/otherAssignments/staffPersonAssignment/schoolRefId
							return $school
							]]>
						</xquery>
					</servicePath>
					<servicePath>
						<path>xStudents/{}/xSchools</path>
						<description>Returns information for all schools in which the specified student is enrolled.</description>
						<xquery>
							<![CDATA[
							declare default element namespace "http://www.sifassociation.org/datamodel/na/3.3";
							for $student in /xStudent[@refId="{}"]
							for $school in /xSchool
							where $school/@refId = $student/enrollment/schoolRefId or $school/@refId = $student/otherEnrollments/enrollment/schoolRefId
							return $school
							]]>
						</xquery>
					</servicePath>
					<servicePath>
						<path>xContacts/{}/xSchools</path>
						<description>Returns information for all schools associated with the contact specified.</description>
						<xquery>
							<![CDATA[
							declare default element namespace "http://www.sifassociation.org/datamodel/na/3.3";
							let $refIdList :=
								for $contact in /xContact[@refId="{}"]
								for $student in /xStudent
								where $student/@refId = $contact/relationships/relationship/studentRefId
								return $student/@refId
							for $school in /xSchool
							for $studentRefId in distinct-values($refIdList)
							for $student in /xStudent[@refId = $studentRefId]
							where $school/@refId = $student/enrollment/schoolRefId or $school/@refId = $student/otherEnrollments/enrollment/schoolRefId
							return $school
							]]>
						</xquery>
					</servicePath>
				</servicePaths>
			</xs:appinfo>
			<xs:documentation>xSchool is a composite object that provides essential information about a school building or campus. The refId of a xSchool object coincides with the refId of a corresponding school object.</xs:documentation>
		</xs:annotation>
